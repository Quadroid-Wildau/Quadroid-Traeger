public class ObserverHandler 
{
	/**reference of this class for singleton*/
	private static ObserverHandler oh = null;
	
	/**no public constructor
	 * for singleton*/
	private ObserverHandler()
	{
		oh = this;
	}
	
	protected void notification()
	{
		
	}
	
}
